<div class="row" >

<div class="col-sm-4">

</div>

<div class="col-sm-4" style="border:1px solid #eee; box-shadow:2px 2px 2px 2px #eee; border-radius:10px; ">
<h2 style="">Sign up</h2>
<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs" >
    <p><%= f.input :email, required: true, autofocus: true ,class: 'abc' %><br /><br /></p>
    <p><%= f.input :password, required: true, hint: ("#{@minimum_password_length} characters minimum" if @minimum_password_length) %><br /><br /></p>
<p>    <%= f.input :password_confirmation, required: true %><br /><br /></p>
  </div>

  <div class="form-actions" >
    <%= f.button :submit, "Sign up" , class: 'btn btn-warning ' %>

<% end %>
<%= render "devise/shared/links" %>
  </div>
<br />



</div>

<div class="col-sm-4">

</div>

</div>